              "2 -79.891803  40.486625  \n",
              "3 -80.001939  40.454080  \n",
              "4 -80.052204  40.445900  "
            ]
          },
          "execution_count": 7,
          "metadata": {},
          "output_type": "execute_result"
        }
      ],
      "source": [
        "# Clean a few fields\n",
        "work = df.copy()\n",
        "if NEIGHBORHOOD_COL is not None:\n",
        "    work[NEIGHBORHOOD_COL] = (work[NEIGHBORHOOD_COL]\n",
        "        .astype(str).str.strip().str.replace(r'\\s+',' ', regex=True).str.title())\n",
        "work.head()"
      ]
    },
    {
      "cell_type": "markdown",
      "metadata": {},
      "source": [
        "```python\n",
        "# Compute Safety Score (0–100): fewer arrests → higher score\n",
        "nb_counts = work.groupby(NEIGHBORHOOD_COL).size().rename('Arrests').sort_values(ascending=False)\n",
        "mm = (nb_counts - nb_counts.min()) / (nb_counts.max() - nb_counts.min())\n",
        "safety = ((1 - mm) * 100).round(2)\n",
        "scores = pd.DataFrame({'Neighborhood': nb_counts.index, 'Arrests': nb_counts.values, 'Safety Score (0-100)': safety.values})\n",
        "scores.head()\n",
        "```\n"
      ]
